<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Zip_Code_Rule</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(REGEX( Zip_Code__c , &quot;\\d{5}(-\\d{4})?&quot;))</errorConditionFormula>
    <errorMessage>Incorrect Zip Code Type</errorMessage>
</ValidationRule>
